{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*\\.html)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*\\.js)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*\\.css)",
            "dest": "/public/$1"
        },
        {
            "src": "/",
            "dest": "/public/student.html"
        },
        {
            "src": "/admin",
            "dest": "/public/admin.html"
        },
        {
            "src": "/grader",
            "dest": "/public/grader.html"
        }
    ]
}